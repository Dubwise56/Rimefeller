  <RecipeDef>
    <defName>Make_Napalm</defName>
    <label>make Napalm</label>
    <description>Make a batch of napalm by mixing chemfuel and synthylene. (Machining Table)</description>
    <jobString>Mixing napalm.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>2000</workAmount>
    <workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Synthylene</li>
          </categories>
        </filter>
        <count>25</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Chemfuel</li>
      </thingDefs>
      <categories>
        <li>Synthylene</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <Napalm>50</Napalm>
    </products>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <researchPrerequisite>Napalm</researchPrerequisite>
  </RecipeDef>

